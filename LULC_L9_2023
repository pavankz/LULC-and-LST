                  [78.50941574051807, 17.381913595698943],
                  [78.50790621754098, 17.383113165658333],
                  [78.5060179423945, 17.3832974645632]]]),
            {
              "LULC": 5,
              "system:index": "14"
            })]),
    imageVisParam = {"opacity":1,"bands":["classification"],"min":0,"max":5,"palette":["2d46cf","5ef705","092e05","e65319","dce619"]};
/***** End of imports. If edited, may not auto-convert in the playground. *****/
Map.addLayer(aoi);
Map.centerObject(aoi, 11);

// Applies scaling factors.
function applyScaleFactors(image) {
  var opticalBands = image.select('SR_B.').multiply(0.0000275).add(-0.2);
  var thermalBands = image.select('ST_B.*').multiply(0.00341802).add(149.0);
  return image.addBands(opticalBands, null, true)
              .addBands(thermalBands, null, true);
}
//Adding Landsat
var dataset = ee.ImageCollection('LANDSAT/LC09/C02/T1_L2')
  .filterDate('2023-01-01', '2023-03-01')
  .filterMetadata('CLOUD_COVER', 'less_than', 0.1)
  .filterBounds(aoi)
  .map(applyScaleFactors)
  .first();
  
print(dataset);

//Clipping of AOI
var Landsat = dataset.clip(aoi);

//FCC Visualization
Map.addLayer(Landsat, {bands:['SR_B5', 'SR_B4', 'SR_B3'], min:0.0, max:0.5, gamma:1.4}, 'FCC')

//Merge Training Polygons into one feature collection
var lulc = Waterbody.merge(Urban_GreenSpace).merge(Aqua_Vegetation).merge(Urban_Builtup).merge(Others);
print(lulc);

//Selecting Bands
 var bands = ['SR_B2', 'SR_B3', 'SR_B4', 'SR_B5', 'SR_B6', 'SR_B7'];
 
 //Feature collection of training data
var training = Landsat.select(bands).sampleRegions({
  collection: lulc,
  properties: ['LULC'],
  scale: 30
});
//Selection of training data

var withRandom = training.randomColumn('random');
var split = 0.7; //70% training, 30% testing

var trainingPartition = withRandom.filter(ee.Filter.lt('random',split));
var testingPartition = withRandom.filter(ee.Filter.gte('random',split));

//Training the Classifier

var classifier = ee.Classifier.smileRandomForest(100).train({
  features: trainingPartition,
  classProperty: 'LULC',
  inputProperties: bands});
  
var final_LU = Landsat.classify(classifier);

 //Color Palette
// var color_palette = [
  // '43f00e', //Waterbody
   //'', //Urban_GreenSpace
   //'0f02f7', //Aqua_Vegetation
   //'dce619', //Urban_Builtup
   //'e65319', //Others
   //];
   
   //Displaying the classified result
   
Map.addLayer(final_LU, {min:1, max:5, palette:['blue', 'green', 'pink', 'red', 'yellow']}, 'aoi');

//Exporting 
Export.image.toDrive({
  image: final_LU,
  description: 'LULC_2023',
  folder: 'LandCover',
  region: aoi,
  crs: 'EPSG:32644',
  scale: 30,
  fileFormat:'GeoTIFF',
  maxPixels:1e12
});

//Accuracy Assessment

var accuracyClassifier = ee.Classifier.smileRandomForest(100).train({
  features: testingPartition,
  classProperty:'LULC',
  inputProperties: bands
});

//Classify the test FeatureCollection
var test = testingPartition.classify(accuracyClassifier);

//Calculate confusiion matrix
var confusionMatrix = test.errorMatrix('LULC', 'classification');
print('Confusion Matrix', confusionMatrix);

//Claculate overall accuracy
var OA = confusionMatrix.accuracy();
print("Overall accuracy", OA);

//calculate consumers accuracy, also known as user's accuracy or
//specificity and the complement of comission error(1-comission error).
print("Consumer's accuracy", confusionMatrix.consumersAccuracy());


//Claculate producers accuracy also known as sensitivity and the
//compliment of omission error(1-omission error).

print("Producer's accuracy", confusionMatrix.producersAccuracy());

//calculate kappa statistic
print('kappa statistic', confusionMatrix.kappa());

// Calculation area fro each class (of classified)

var classes_area = ee.Image.pixelArea().addBands(final_LU).divide(1e6).reduceRegion({
  reducer: ee.Reducer.sum().group(1), geometry: aoi, scale: 30, bestEffort:true});
  
print("All Classes Area in Sqare Km", classes_area);